{
    "name": "excache",
    "version": "2.0.0",
    "description": "Extensible Cache",
    "type": "module",
    "main": "out/main/index.js",
    "exports": {
        ".": "./out/main/index.js"
    },
    "files": [
        "out/main/**/*"
    ],
    "scripts": {
        "clean": "rm -rf out *.tsbuildinfo",
        "dev": "tsc -b -w",
        "compile": "tsc -b",
        "lint": "eslint --ext=.js,.ts --cache .",
        "test": "NODE_ENV=test mocha",
        "preversion": "npm run lint",
        "version": "npm run compile",
        "postversion": "npm publish --access=public && git push --tags origin main"
    },
    "pre-commit": [
        "lint"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/inca/excache.git"
    },
    "keywords": [
        "cache",
        "extensible",
        "lru",
        "promise"
    ],
    "author": "Boris Okunskiy",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/inca/excache/issues"
    },
    "homepage": "https://github.com/inca/excache#readme",
    "devDependencies": {
        "@types/mocha": "^9.1.1",
        "@types/node": "^20.8.2",
        "@ubio/eslint-config": "^1.3.8",
        "eslint": "^7.32.0",
        "mocha": "^10.0.0",
        "pre-commit": "^1.2.2",
        "typescript": "^5.9.3"
    }
}
